@model List<WebApplication2.Models.Channel>

@if (ViewBag.RSSFeed != null)
{
    @*<div>@ViewBag.RSSFeed[0].Title</div>*@
    <div>@Model[0].Title</div>
}
<table class="table table-hover">
    <thead>
        <tr>
            <th>Title</th>
            <th>Link</th>
            <th>Description</th>
            <th>Publish Date</th>
        </tr>
    </thead>
    <tbody>
        @if (ViewBag.RSSFeed != null)
        {
            foreach (var item1 in ViewBag.RSSFeed)
            {
                foreach (var item in item1.Item)
                {
                <tr>
                    <td>@item.Title</td>
                    <td><a href="@item.Link">@item.Link</a></td>
                    <td>@Html.Raw(item.Description)</td>
                    <td>@item.PubDate</td>
                </tr>
                }
            }
        }
    </tbody>
</table>
  
